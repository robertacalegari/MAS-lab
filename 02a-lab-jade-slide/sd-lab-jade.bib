@incollection{spatialcomputinglanguages-dslbook2013,
	Author = {Beal, Jacob and Dulman, Stefan and Usbeck, Kyle and Viroli, Mirko and Correll, Nikolaus},
	Booktitle = {Formal and Practical Aspects of Domain-Specific Languages},
	Date-Added = {2016-11-17 18:56:21 +0000},
