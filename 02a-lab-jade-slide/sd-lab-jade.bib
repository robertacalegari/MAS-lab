@article{spacelogic-synthese186,
	Author = {Aiello, Marco and Bezhanishvili, Guram and Bloch, Isabelle and Goranko, Valentin},
	Date-Added = {2016-11-17 16:03:36 +0000},
	Date-Modified = {2016-11-17 16:03:36 +0000},
	Doi = {10.1007/s11229-011-9913-9},
	Isbn = {1122901199139},
	Issn = {00397857},
	Journal = {Synthese},
	Keywords = {Geometry,Mathematical morphology,Modal logic,Topology},
	Number = 3,
	Pages = {619--632},
	Publisher = {Springer Netherlands},
	Shorttitle = {Logic for physical space},
	Title = {Logic for physical space: From antiquity to present days},
	Url = {http://link.springer.com/10.1007/s11229-011-9913-9},
	Volume = 186,
	Year = 2012,
	Abstract = {Since the early days of physics, space has called for means to represent, experiment, and reason about it. Apart from physicists, the concept of space has intrigued also philosophers, mathematicians and, more recently, computer scientists. This longstanding interest has left us with a plethora of mathematical tools developed to represent and work with space. Here we take a special look at this evolution by considering the perspective of Logic. From the initial axiomatic efforts of Euclid, we revisit the major milestones in the logical representation of space and investigate current trends. In doing so, we do not only consider classical logic, but we indulge ourselves with modal logics. These present themselves naturally by providing simple axiomatizations of different geometries, topologies, space-time causality, and vector spaces.},
	Bdsk-Url-1 = {http://dx.doi.org/10.1007/s11229-011-9913-9}}

